<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Walmart - Dashboard</title>
    </head>
    <body>
        <h1>Items</h1>
        <ul>
            <% items.map((item) => { %>
                <li>
                    <%= item.name %>, <%= item.description %>, <%= item.price %>, <%= item.quantity %>
                </li>
            <% }); %>
        </ul>
    </body>
</html>